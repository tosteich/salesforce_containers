<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Serial_Number_start_from_STA</fullName>
    <active>true</active>
    <description>Ship Serial Number should start or contain “STA-”</description>
    <errorConditionFormula>NOT CONTAINS(Serial_Number__c, &quot;STA-&quot;)</errorConditionFormula>
    <errorDisplayField>Serial_Number__c</errorDisplayField>
    <errorMessage>Ship Serial Number should start or contain “STA-”</errorMessage>
</ValidationRule>
